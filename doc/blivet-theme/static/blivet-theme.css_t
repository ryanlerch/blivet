@import url("default.css");

body > div
{
	max-width:1200px;
	min-width:1000px;
	margin-left:auto;
	margin-right:auto;
}
#header
{
	background-color:#5b9391;
	height:80px;
	width:100%;
}

#subheader
{
	background-color:#444;
	height:100px;
	width:100%;
	padding-left:30px;
	line-height:100px;
	box-sizing:border-box;
	color:white;
	font-size:30px;
}

/*the top breadcrumb bar*/
div.related
{
	background-color:white;
	color:#444;
}

div.related a
{
	color:#444;
}
